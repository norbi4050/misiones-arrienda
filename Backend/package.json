    "@types/leaflet.markercluster": "^1.5.6",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "postcss": "^8.4.38",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
