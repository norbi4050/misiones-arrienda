    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lucide-react": "^0.294.0",
    "mercadopago": "^2.0.15",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.9.8",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "^5.0.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
