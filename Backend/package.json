{
  "name": "misiones-arrienda",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prisma generate && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:deploy": "prisma migrate deploy && prisma generate",
    "postinstall": "prisma generate",
    "snapshot": "node scripts/make-snapshot.mjs > scripts/snapshot.json",
    "snapshot:diff": "node scripts/diff-snapshot.mjs --prev scripts/snapshot.json --out scripts/change-report.json",
    "smoke:analytics": "powershell -ExecutionPolicy Bypass -File scripts/smoke-tests-analytics.ps1",
    "smoke:win": "powershell -ExecutionPolicy Bypass -File scripts/smoke-tests.ps1",
    "smoke:sprint-d": "powershell -ExecutionPolicy Bypass -File scripts/smoke-tests-sprint-d.ps1",
    "test:avatar": "powershell -ExecutionPolicy Bypass -File scripts/test-avatar-flow.ps1"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
